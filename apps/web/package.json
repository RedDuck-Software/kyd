    "react": "18.2.0",
    "react-blockies": "^1.4.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.0",
    "react-router-dom": "^6.22.3",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "viem": "2.x",
    "wagmi": "^2.10.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
