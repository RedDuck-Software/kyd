{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "format": "yarn format:prettier && yarn format:es",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,d.ts}\"",
    "format:es": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint": "yarn lint:prettier && yarn lint:es",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint:es": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
